{
    "name": "taskmanager/medoo",
    "description": "Task Manager com Medoo",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "catfan/medoo": "^2.1"
    },
    "require-dev": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}